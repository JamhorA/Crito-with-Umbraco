@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockListItem>;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
    var content = (ContentModels.ChooseUs)Model.Content;
}


<div class="item">
    <div class="icon"><i class="@content.Value("chooseUsNewIcon")"></i></div>
    <div>
        <div class="title">@content.Value("chooseUsNewTitle")</div>
        <p class="text">@content.Value("chooseUsNewDescription")</p>
    </div>
</div>
